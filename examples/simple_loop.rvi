    nop
    nop
    lw $1, $0, 0            # loop counter
    lw $2, $0, 1
    lw $3, $0, 2            # loop limit
    lw $4, $0, 3            # fibonacci no. 1
    lw $5, $0, 4            # fibonacci no. 2
LOOP:
    addi $1, $1, 1          #
    add $0, $0, $0
    add $0, $0, $0
    add $0, $0, $0
    beq $3, $1, DONE
    add $0, $0, $0
    add $4, $4, $5
    add $0, $0, $0
    add $0, $0, $0
    add $0, $0, $0
    add $5, $4, $5
    beq $0, $0, LOOP
DONE:
    add $0, $0, $0
