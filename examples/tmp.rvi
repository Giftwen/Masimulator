    addi $3, $0, 100
    addi $0, $0, 0
    addi $0, $0, 0
LOOP:
    beq $3, $1, DONE            # if counter == loop limit then done!
    addi $1, $1, 1              # counter++
    sw $1, $1, 0
    beq $0, $0, LOOP            # jump
DONE:
    addi $0, $0, 0
    addi $0, $0, 0
    addi $0, $0, 0
    addi $0, $0, 0
    addi $0, $0, 0
